  "bugs": {
    "url": "https://github.com/elben10/hugopersonal/issues"
  },
  "homepage": "https://github.com/elben10/hugopersonal#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "extract-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "hugo-bin": "^0.30.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "popper.js": "^1.14.4",
