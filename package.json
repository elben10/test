{
  "name": "hugopersonal",
  "version": "0.1.0",
  "description": "A hugo theme for personal webpage",
  "scripts": {
    "build": "npm run webpack; npm run hugo",
    "hugo": "cd exampleSite; $(npm bin)/hugo --themesDir=../..",
    "hugo-serve": "cd exampleSite; hugo server --themesDir=../..",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch",
    "watch": "npm run hugo-serve & npm run webpack-watch"
  },
